	Keywords = {Costs;Covariance matrix;Design optimization;Eigenvalues and eigenfunctions;Euclidean distance;Helium;Mathematical model;Optimization methods;Performance analysis;Problem-solving},
	Number = {8},
	Pages = {610-616},
	Title = {Cluster Analysis Using Seed Points and Density-Determined Hyperspheres as an Aid to Global Optimization},
	Volume = {7},
	Year = {1977},
