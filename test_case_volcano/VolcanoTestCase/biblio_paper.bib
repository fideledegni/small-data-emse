@phdthesis{Zoutendijk1960,
	Abstract = {This monograph is concerned with the mathematical and computational
	aspects of the linear, quadratic and convex programming problem.
	The most important results of this study are: 1. the development
	of a new computational algorithm for the simplex method, which turns
	out to lead to considerably smaller computer terms than any existing
	algorithm; 2. the development of a number of methods for the convex
	programming problems, called methods of feasible directions, which
	use the simplex method as subroutine ; 3. the establishment of an
	equivalence between many existing methods for the linear, quadratic
	or convex programming problem and the methods of feasible directions.
	The author gives a complete survey of the methods in such a way that
	programming for an electronic computer becomes easy. It may safely
	be stated that another important contribution has been made to this
	rapidly growing offshoot of the mathematical sciences.},
	Author = {Zoutendijk, G.},
	Date-Added = {2013-06-25 08:33:54 +0000},
	Date-Modified = {2013-07-16 14:25:47 +0000},
	School = {University of Amsterdam},
	Title = {Methods of feasible directions: A study in linear and non-linear programming},
	Year = {1960}}
