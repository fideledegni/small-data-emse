	Issn = {0045-7949},
	Journal = {Computers \& Structures},
	Keywords = {Response surface methodology},
	Number = {25--26},
	Pages = {2121 - 2136},
	Title = {A comparative study of metamodeling methods for multiobjective crashworthiness optimization},
	Url = {http://www.sciencedirect.com/science/article/pii/S0045794905001355},
	Volume = {83},
	Year = {2005},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0045794905001355},
