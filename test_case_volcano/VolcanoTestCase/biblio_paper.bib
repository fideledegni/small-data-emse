	Keywords = {function approximation;learning (artificial intelligence);least squares approximations;quadratic programming;regression analysis;support vector machines;function approximation;function simultaneous learning;linear equation;quadratic programming problem;regularized least squares twin support vector regression;support vector machine-type problem;Equations;Function approximation;Least squares approximation;Least squares methods;Linear systems;Pattern classification;Quadratic programming;Statistical learning;Support vector machine classification;Support vector machines;Support Vector regression;Support vector machines;function approximation;least squares approximations},
	Pages = {1192-1197},
