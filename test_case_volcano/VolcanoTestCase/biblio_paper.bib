	Journal = {European Journal of Operational Research},
	Keywords = {Traveling salesman problem},
	Number = {1},
	Pages = {77 - 92},
	Title = {A theoretical study on the behavior of simulated annealing leading to a new cooling schedule},
	Url = {http://www.sciencedirect.com/science/article/pii/S0377221704003388},
	Volume = {166},
